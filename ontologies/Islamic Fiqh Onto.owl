            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/منفذة_على"/>
                <owl:allValuesFrom rdf:resource="http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/شروط_الحج"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/يشترط_وجوب"/>
